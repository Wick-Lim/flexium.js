{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020", "DOM"],
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "jsxImportSource": "flexium",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "allowImportingTsExtensions": true,
    "paths": {
      "flexium/core": ["../../packages/flexium/src/core.ts"],
      "flexium/dom": ["../../packages/flexium/src/dom.ts"],
      "flexium/canvas": ["../../packages/flexium/src/canvas.ts"],
      "flexium/game": ["../../packages/flexium/src/game.ts"],
      "flexium/jsx-runtime": ["../../packages/flexium/src/jsx-runtime.ts"],
      "flexium/jsx-dev-runtime": ["../../packages/flexium/src/jsx-dev-runtime.ts"],
      "flexium": ["../../packages/flexium/src/index.ts"],
      "../../packages/flexium/dist/core.mjs": ["../../packages/flexium/dist/core.d.ts"],
      "../../packages/flexium/dist/dom.mjs": ["../../packages/flexium/dist/dom.d.ts"],
      "../../packages/flexium/dist/canvas.mjs": ["../../packages/flexium/dist/canvas.d.ts"],
      "../../packages/flexium/dist/game.mjs": ["../../packages/flexium/dist/game.d.ts"],
      "../../packages/flexium/dist/index.mjs": ["../../packages/flexium/dist/index.d.ts"],
      "../../packages/flexium/dist/jsx-runtime.mjs": ["../../packages/flexium/dist/jsx-runtime.d.ts"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "../../packages/flexium/src/**/*.ts", "../../packages/flexium/src/**/*.d.ts"],
  "exclude": ["node_modules"]
}
