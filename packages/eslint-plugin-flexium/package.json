{
  "name": "eslint-plugin-flexium",
  "version": "0.12.11",
  "description": "ESLint plugin for Flexium with rules for best practices",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "flexium",
    "signals",
    "reactive",
    "linter"
  ],
  "author": "Wick Lim",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wick-Lim/flexium.js.git",
    "directory": "packages/eslint-plugin-flexium"
  },
  "bugs": {
    "url": "https://github.com/Wick-Lim/flexium.js/issues"
  },
  "homepage": "https://flexium.junhyuk.im",
  "devDependencies": {
    "@types/eslint": "^8.56.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/utils": "^6.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  }
}
