{
  "name": "vite-plugin-flexium",
  "version": "0.8.1",
  "description": "Vite plugin for Flexium - JSX transform, HMR, and dev tools integration",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "flexium",
    "signals",
    "reactive",
    "jsx",
    "hmr"
  ],
  "author": "Wick Lim",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wick-Lim/flexium.js.git",
    "directory": "packages/vite-plugin-flexium"
  },
  "bugs": {
    "url": "https://github.com/Wick-Lim/flexium.js/issues"
  },
  "homepage": "https://flexium.junhyuk.im",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "vite": ">=4.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-syntax-jsx": "^7.24.0"
  }
}
